<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SYS_MPILevel" table="SYS_MPILevel">
		<id name="DCID" type="java.lang.Integer">
			<column name="DCID" />
			<generator class="sequence">
				<param name="sequence">SEQ_SYS_MPILevel</param>
			</generator>
		</id>
		<property name="mpiId" type="java.lang.String" length="32" />
		<property name="mpiLevel" type="java.lang.String" length="1"/>
	</class>
</hibernate-mapping>
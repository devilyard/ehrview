<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="EHR_HealthRecord" table="EHR_HealthRecord">
			<id name="DCID" type="java.lang.String">
            	<column name="DCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="SourceID" type="java.lang.String" length="50" />
        	<property name="RecordClassifying" type="java.lang.String" length="50" />
        	<property name="RecordTitle" type="java.lang.String" length="50" />
        	<property name="EffectiveTime" type="java.util.Date"  />
        	<property name="EffectiveFlag" type="java.lang.String" length="1" />
        	<property name="VersionNumber" type="java.lang.String" length="10" />
        	<property name="AuthorOrganization" type="java.lang.String" length="16" />
        	<property name="Author" type="java.lang.String" length="32" />
        	<property name="SystemTime" type="java.util.Date"  />
        	<property name="UploadTime" type="java.util.Date"  />
        	<property name="UpdateTime" type="java.util.Date"  />
        	<property name="PHRID" type="java.lang.String" length="16" />
        	<property name="FamilyID" type="java.lang.String" length="16" />
        	<property name="MotherID" type="java.lang.String" length="32" />
        	<property name="PartnerID" type="java.lang.String" length="32" />
        	<property name="FatherID" type="java.lang.String" length="32" />
        	<property name="RelaCode" type="java.lang.String" length="2" />
        	<property name="ManaDoctor_Text" type="java.lang.String" length="30" />
        	<property name="ManaDoctorID" type="java.lang.String" length="20" />
        	<property name="RegionCode_Text" type="java.lang.String" length="60" />
        	<property name="RegionCode" type="java.lang.String" length="25" />
        	<property name="ManaUnitID" type="java.lang.String" length="16" />
        	<property name="ManaUnit_Text" type="java.lang.String" length="60" />
</class>
</hibernate-mapping>
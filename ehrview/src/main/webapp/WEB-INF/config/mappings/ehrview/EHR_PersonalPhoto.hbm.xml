<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="EHR_PersonalPhoto" table="EHR_PersonalPhoto">
			<id name="DCID" type="java.lang.String">
            	<column name="DCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="Uploader" type="java.lang.String" length="32" />
        	<property name="UploadTime" type="java.util.Date"  />
        	<property name="DataSource" type="java.lang.String" length="50" />
        	<property name="DocContent" type="java.lang.String"  />
</class>
</hibernate-mapping>
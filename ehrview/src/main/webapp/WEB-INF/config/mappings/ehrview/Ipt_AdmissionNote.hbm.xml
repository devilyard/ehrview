<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="Ipt_AdmissionNote" table="Ipt_AdmissionNote">
			<id name="DCID" type="java.lang.String">
            	<column name="DCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="SourceID" type="java.lang.String" length="50" />
        	<property name="RecordClassifying" type="java.lang.String" length="50" />
        	<property name="RecordTitle" type="java.lang.String" length="50" />
        	<property name="EffectiveTime" type="date"  />
        	<property name="EffectiveFlag" type="java.lang.String" length="1" />
        	<property name="VersionNumber" type="java.lang.String" length="10" />
        	<property name="AuthorOrganization" type="java.lang.String" length="16" />
        	<property name="Author" type="java.lang.String" length="32" />
        	<property name="SystemTime" type="date"  />
        	<property name="UploadTime" type="date"  />
        	<property name="UpdateTime" type="date"  />
        	<property name="JZLSH" type="java.lang.String" length="32" />
        	<property name="BZBAH" type="java.lang.String" length="18" />
        	<property name="BZZS" type="java.lang.String" length="100" />
        	<property name="BZXBS" type="java.lang.String" length="2000" />
        	<property name="BZJWS" type="java.lang.String" length="2000" />
        	<property name="RYRQSJ" type="date"  />
        	<property name="BZXZZD" type="java.lang.String" length="500" />
</class>
</hibernate-mapping>
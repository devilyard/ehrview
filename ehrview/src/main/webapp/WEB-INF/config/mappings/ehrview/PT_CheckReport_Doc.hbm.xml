<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="PT_CheckReport_Doc" table="PT_CheckReport_Doc">
			<id name="DocID" type="java.lang.String">
            	<column name="DocID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="DCID" type="java.lang.String" length="16" />
        	<property name="DocFormat" type="java.lang.String" length="2" />
        	<property name="DocContent" type="binary"  />
        	<property name="CompressionAlgorithm" type="java.lang.String" length="20"/>
</class>
</hibernate-mapping>
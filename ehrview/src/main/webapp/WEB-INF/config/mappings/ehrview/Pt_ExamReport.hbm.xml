<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="Pt_ExamReport" table="Pt_ExamReport">
			<id name="DCID" type="java.lang.String">
            	<column name="DCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="SourceID" type="java.lang.String" length="50" />
        	<property name="RecordClassifying" type="java.lang.String" length="50" />
        	<property name="RecordTitle" type="java.lang.String" length="50" />
        	<property name="EffectiveTime" type="date"  />
        	<property name="EffectiveFlag" type="java.lang.String" length="1" />
        	<property name="VersionNumber" type="java.lang.String" length="10" />
        	<property name="AuthorOrganization" type="java.lang.String" length="16" />
        	<property name="AuthorOrganization_TEXT" type="java.lang.String" length="100" />
        	<property name="Author" type="java.lang.String" length="32" />
        	<property name="SystemTime" type="date"  />
        	<property name="UploadTime" type="date"  />
        	<property name="UpdateTime" type="date"  />
        	<property name="JZLSH" type="java.lang.String" length="32" />
        	<property name="JCLSH" type="java.lang.String" length="32" />
        	<property name="BRBSLB" type="java.lang.String" length="2" />
        	<property name="SQDH" type="java.lang.String" length="32" />
        	<property name="KDSJ" type="date"  />
        	<property name="JCLX" type="java.lang.String" length="2" />
        	<property name="JCRQ" type="date"  />
        	<property name="BGSJ" type="date"  />
        	<property name="SHRQ" type="date"  />
        	<property name="BRBSLB_TEXT" type="java.lang.String" length="16" />
        	<property name="BCKSMC" type="java.lang.String" length="70" />
        	<property name="JCXMMC" type="java.lang.String" length="60" />
        	<property name="StudyUID" type="java.lang.String" length="100" />
        	
</class>
</hibernate-mapping>
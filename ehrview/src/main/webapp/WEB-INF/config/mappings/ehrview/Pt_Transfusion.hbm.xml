            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="SOURCEID" type="java.lang.String" length="50" />
        	<property name="RecordClassifying" type="java.lang.String" length="50" />
        	<property name="RecordTitle" type="java.lang.String" length="50" />
        	<property name="EffectiveTime" type="java.util.Date"  />
        	<property name="EffectiveFlag" type="java.lang.String" length="1" />
        	<property name="VersionNumber" type="java.lang.String" length="10" />
        	<property name="AuthorOrganization" type="java.lang.String" length="22" />
        	<property name="AuthorOrganization_Text" type="java.lang.String" length="50" />
        	<property name="Author" type="java.lang.String" length="32" />
        	<property name="SystemTime" type="java.util.Date" />
        	<property name="UploadTime" type="java.util.Date" />
        	<property name="UpdateTime" type="java.util.Date" />
        	<property name="JZLSH" type="java.lang.String" length="32" />
        	<property name="SXLSH" type="java.lang.String" length="32" />
        	<property name="SXSJ" type="java.util.Date" />
        	<property name="SXLX" type="java.lang.String" length="1" />
        	<property name="SXLX_TEXT" type="java.lang.String" length="50" />
        	<property name="SXFY" type="java.lang.String" length="1" />
        	<property name="SXFY_TEXT" type="java.lang.String" length="10" />
        	<property name="SXL" type="integer" length="10" />
</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="SUMMARY_HIST_HOSPITAL" table="SUMMARY_HIST_HOSPITAL">
			<id name="DCID" type="java.lang.String">
            	<column name="DCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="DIAGNOSIS" type="java.lang.String" length="200" />
        	<property name="DIAGNOSISDATE" type="date" />
        	<property name="DIAGNOSISUNITID" type="java.lang.String" length="16" />
        	<property name="DIAGNOSISUNIT_TEXT" type="java.lang.String" length="200"  />
        	<property name="UPLOADTIME" type="date"  />
        	<property name="SYSTEMTIME" type="date"  />
        	<property name="AUTHORORGANIZATION" type="java.lang.String" length="200" />
        	<property name="SRCENTRYNAME" type="java.lang.String" length="200" />
        	<property name="SRCID" type="java.lang.String" length="200" />
        	<property name="EFFECTIVEFLAG" type="java.lang.String" length="1" />
        	<property name="TRANSFLAG" type="java.lang.String" length="1" />
        	<property name="UPDATETIME" type="date"  />
        	<property name="SOURCEFLAG" type="java.lang.String" length="1" />
        	<property name="HEADER" type="java.lang.String" length="4000" />
</class>
</hibernate-mapping>
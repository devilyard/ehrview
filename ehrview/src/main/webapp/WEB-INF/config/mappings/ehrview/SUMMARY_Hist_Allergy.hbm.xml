            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="UploadTime" type="date"  />
        	<property name="SystemTime" type="date"  />
        	<property name="EffectiveFlag" type="java.lang.String" length="1" />
        	<property name="AuthorOrganization" type="java.lang.String" length="16" />
        	<property name="AuthorOrganization_Text" type="java.lang.String" length="200" />
        	<property name="SRCEntryName" type="java.lang.String" length="50" />
        	<property name="SRCID" type="java.lang.String" length="50" />
        	<property name="AllergenCode" type="java.lang.String" length="30" />
        	<property name="AllergenName" type="java.lang.String" length="50" />
        	<property name="AllergyMedicineCode" type="java.lang.String" length="10" />
        	<property name="AllergyMedicineName" type="java.lang.String" length="50" />
        	<property name="AllergyMedicineCategory" type="java.lang.String" length="10" />
        	<property name="AllergyMedicineCategoryNm" type="java.lang.String" length="50" />
</class>
</hibernate-mapping>
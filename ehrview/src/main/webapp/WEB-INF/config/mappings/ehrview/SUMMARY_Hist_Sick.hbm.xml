<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="SUMMARY_Hist_Sick" table="SUMMARY_Hist_Sick">
			<id name="DCID" type="java.lang.String">
            	<column name="DCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="MPIID" type="java.lang.String" length="32" />
        	<property name="Diagnosis" type="java.lang.String" length="200" />
        	<property name="DiagnosisDate" type="date"  />
        	<property name="DiagnosisUnitID" type="java.lang.String" length="16" />
        	<property name="DiagnosisUnit_Text" type="java.lang.String" length="200" />
        	<property name="UploadTime" type="date"  />
        	<property name="SystemTime" type="date"  />
        	<property name="AuthorOrganization" type="java.lang.String" length="16" />
        	<property name="SRCEntryName" type="java.lang.String" length="50" />
        	<property name="SRCID" type="java.lang.String" length="50" />
        	<property name="EffectiveFlag" type="java.lang.String" length="1" />
</class>
</hibernate-mapping>
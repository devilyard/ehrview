        	<property name="DIAGNOSISUNIT_TEXT" type="java.lang.String" length="200" />
        	<property name="UPLOADTIME" type="date" />
        	<property name="SYSTEMTIME" type="date" />
        	<property name="AUTHORORGANIZATION" type="java.lang.String" length="22" />
        	<property name="SRCENTRYNAME" type="java.lang.String" length="50" />
        	<property name="SRCID" type="java.lang.String" length="50" />
        	<property name="DIAGNOSISUNITTEXT" type="java.lang.String" length="200" />

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="EMR_TransFormError" table="EMR_TransFormError">
		<id name="DCID" type="java.lang.Integer">
           	<column name="DCID"/>
           	<generator class="sequence">
				<param name="sequence">EMR_TRANSFORMERROR_SQ</param>
			</generator>
        </id>
		<property name="UploadDate" type="java.util.Date" length="7" />
 		<property name="ErrorMsg" type="java.lang.String"/>
       	<property name="RecordClassifying" type="java.lang.String" length="50" />
        <property name="Name" type="java.lang.String" length="20" />
        <property name="IdCard" type="java.lang.String" length="20" />
        <property name="MPIID" type="java.lang.String" length="32" />
	</class>
</hibernate-mapping>
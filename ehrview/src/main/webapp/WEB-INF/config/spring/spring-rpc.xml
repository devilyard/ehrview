		<property name="acceptableVersion" value="2011_V1.0"/>
		<property name="sourceFormat" value="02"/>
		<property name="destFormat" value="11"/>
		<property name="templateEngine" ref="htmlTemplateEngine"/>
	</bean>
	<bean id="commonThymeleafTemplateFormater2012" class="com.bsoft.xds.support.instance.CommonThymeleafTemplateFormater">

		<ehr:property name="dao" ref="serviceDao"/>
		<ehr:property name="maxLimit" value="100"/>
		<ehr:property name="maxPeriodDays" value="30"/>
		<ehr:property name="templateFormaters">
			<ehr:list>
				<ehr:ref bean="commonThymeleafTemplateFormater"/>
				<ehr:ref bean="commonThymeleafTemplateFormater2012"/>
			</ehr:list>
		</ehr:property>
		<ehr:property name="documentDecompressors">
			<ehr:list>
				<ehr:ref bean="unzipDocumentCompressor"/>	
				<ehr:ref bean="gzipDocumentCompressor"/>
			</ehr:list>
		</ehr:property>
	</ehr:service>
